<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - KajalEnterprises</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/KajalEnterprises.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-dark bg-primary border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">KajalEnterprises</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link " asp-area="Customer" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link " asp-area="Customer" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                        <li class="nav-item navbar-nav  dropdown">
                            <a class="nav-link dropdown-toggle " asp-area="Admin" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Content Management
                            </a>
                            <ul class="dropdown-menu">
                                <li><a class="dropdown-item nav-link text-dark" asp-area="Admin" asp-controller="Category" asp-action="Index">Category</a></li>
                                <li><a class="dropdown-item nav-link text-dark" asp-area="Admin" asp-controller="Product" asp-action="Index">Product</a></li>
                            </ul>
                        </li>
                    </ul>
                    <partial name="_LoginPartial"/>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            <partial name="_Notification" />
            @RenderBody()
        </main>
    </div>

    <div class="container border-top pb-3">
        <div class="row">
            <div class="col-md-4 mt-3">
                <a class="nav-link " asp-area="Customer" asp-controller="Home" asp-action="AboutUs"><h6>About Us</h6></a>
                <a class="nav-link " asp-area="Customer" asp-controller="Home" asp-action="ContactUs"><h6>Contact Us</h6></a>
                <a class="nav-link " asp-area="Customer" asp-controller="Home" asp-action="Privacy"><h6>Privacy</h6></a>
            </div>
            <div class="col-md-4">

                <p>
                    <div class="text-dark-emphasis"> <h6>Mail Us:</h6></div>
                    Kajal Enterprises,<br>
                    H-159, Street-5, 70ft Road,<br>
                    Premnagar-II, Kirari Suleman Nagar,<br>
                    Delhi-110086, Delhi, India
                </p>
            </div>
            <div class="col-md-4">
                <p>
                    <div class="text-dark-emphasis"> <h6>Office Address:</h6></div>
                    Kajal Enterprises,<br>
                    H-159, Street-5, 70ft Road,<br>
                    Premnagar-II, Kirari Suleman Nagar,<br>
                    Delhi-110086, Delhi, India
                </p>
                <p>
                    Contact:
                    <a href="tel:+9205540465">+9205540465</a>, <a href="tel:+918287201284">+918287201284</a>
                </p>
            </div>

        </div>
        <hr>
        <div class="col-md-6">
            <h5 class="d-inline">Social :&nbsp;&nbsp;</h5>
            <ul class="list-inline d-inline">
                <li class="list-inline-item"><a href="https://wa.me/8287201284"><img src="/images/Whatsapp-Logo.png" alt="WhatsApp" width="30" height="30"></a></li>
                <li class="list-inline-item"><a href="https://www.facebook.com/profile.php?id=61560415064925&mibextid=ZbWKwL" target="_blank"><img src="/images/Facebook-Logo.png" alt="Facebook" width="25" height="25"></a></li>
                <li class="list-inline-item"><a href="https://www.instagram.com/kajalenterprisess?igsh=bTZxbzNsNnM2a3A4" target="_blank"><img src="/images/Instagram-Logo.png" alt="Instagram" width="30" height="30"></a></li>
                <li class="list-inline-item"><a href="mailto:kajalenterprises8287@gmail.com"><img src="/images/Gmail-Logo.png" alt="Email" width="30" height="30"></a></li>

            </ul>
        </div>

    </div>
    <footer class="border-top footer bg-primary text-muted ">
        <div class="container">
            &copy; 2024 - KajalEnterprises - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
